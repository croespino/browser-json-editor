{
  "manifest_version": 3,
  "name": "JSON Editor",
  "description": "A tool to view, edit, format, repair, query, transform, and validate JSON",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}